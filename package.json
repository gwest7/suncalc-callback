{
  "name": "@binaryme/suncalc-callback",
  "version": "1.0.0",
  "description": "A callback/promise approach to SunCalc",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwest7/suncalc-callback.git"
  },
  "keywords": ["SunCalc"],
  "author": "gwest7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gwest7/suncalc-callback/issues"
  },
  "homepage": "https://github.com/gwest7/suncalc-callback#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/suncalc": "^1.8.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "suncalc": "^1.9.0",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  }
}
